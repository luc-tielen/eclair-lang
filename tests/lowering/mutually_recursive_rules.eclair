          }
          d.iter_next(begin_iter_5)
        }
        range_query.end_3:
        c.iter_next(begin_iter_4)
      }
      range_query.end_2:
    }
    condition_6 = new_b.is_empty(FN_ARG[0].7)
    if (condition_6)
    {
      condition_7 = new_c.is_empty(FN_ARG[0].8)
      if (condition_7)
      {
